{
  "expo": {
    "name": "Great Places app",
    "description": "Take your favourite photos & mark them on google map 'using a dummy Api' to restore your wonderful memories whenever you wish.",
    "slug": "great_places_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/map.png",
    "splash": {
      "image": "./assets/map.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
